# Copy this file to .env and update the values as needed
# cp env.template .env

# Postgres Database Configuration
# In case of running the backend through docker compose put the below value
# DATABASE_LINK=postgresql://myuser:mypassword@postgres:5432/mydatabase
# In case of running the backend through start_postgres.sh put the below value
# DATABASE_LINK=postgresql://myuser:mypassword@localhost:5432/mydatabase
POSTGRES_DATABASE_LINK=
MONGO_DATABASE_LINK=

# JWT Configuration (required)
JWT_SECRET=

# AWS S3 Configuration (optional - S3 features will be disabled if not provided)
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_REGION=
S3_BUCKET=

# Firebase Credentials
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=
FIREBASE_MEASUREMENT_ID=

# Neo4j database

NEO4J_URI=
NEO4J_USERNAME=
NEO4J_PASSWORD=

# Dropbox Configuration (optional - Dropbox features will be disabled if not provided)
DROPBOX_ACCESS_TOKEN=

# Application Configuration
NODE_ENV=

# Default port 3000
PORT=
